    border-radius: 50%;
    color: #fff;
    display: block;
    width: 35px;
    height: 35px;
    line-height: 42px;
    text-align: center;
	top: 0;
	position: absolute;
}
#home-video .arrow-holder:hover,
#home-video .arrow-holder:focus,
#home-video .arrow-holder:active {
	text-decoration: none;
	opacity: 0.9;
}

#home-video .arrow-holder.left { right: 40px; }
#home-video .arrow-holder.right { right: 0; }



#home-video { 
	margin: 20px 0 0 0;
	position: relative;
}
#home-video .post {}

