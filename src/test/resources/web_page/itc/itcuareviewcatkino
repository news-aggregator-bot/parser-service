                googletag.defineSlot('/22614634789/itc_content_premium_728x90', [728, 90], 'div-gpt-ad-itc_content_premium_728x90').addService(googletag.pubads());
                googletag.defineSlot('/22614634789/itc_content_premium1_300x250', [300, 250], 'div-gpt-ad-itc_content_premium1_300x250').addService(googletag.pubads());
                googletag.defineSlot('/22614634789/itc_footer_970x250', [970, 250], 'div-gpt-ad-itc_footer_970x250').addService(googletag.pubads());
                googletag.defineSlot('/22614634789/itc_rightbar_bottom_300x250', [300, 250], 'div-gpt-ad-itc_rightbar_bottom_300x250').addService(googletag.pubads());
                googletag.pubads().enableSingleRequest();
                googletag.pubads().collapseEmptyDivs();
                googletag.enableServices();
            });
        } else {
            window.googletag = window.googletag || {cmd: []};
            googletag.cmd.push(function() {
                googletag.defineSlot('/22614634789/itc.ua_mobile_content_bottom_300x250', [300, 250], 'div-gpt-ad-itc.ua_mobile_content_bottom_300x250').addService(googletag.pubads());
                googletag.defineSlot('/22614634789/itc.ua_mobile_undercontent_300x250', [300, 250], 'div-gpt-ad-itc.ua_mobile_undercontent_300x250').addService(googletag.pubads());
