  <script type="text/javascript">
	function OpenPopupCenter(pageURL, title, w, h) {
		var left = (screen.width - w) / 2
		var top = (screen.height - h) / 4  // for 25% - devide by 4  |  for 33% - devide by 3
		var targetWin = window.open(pageURL, title, 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, copyhistory=no, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left)
	}
</script> 
  <script type="text/javascript">
	var inIframe = function() {
		try {
			return window.self !== window.top;
		} catch (e) {
			return true;}
	},
			img = document.createElement("img"),
			path = "//nc-ru.redtram.com/px/10800.gif";
	if (inIframe()) {
		img.setAttribute("src", path+ "?referrer="+ document.referrer);
	}else {
		img.setAttribute("src", path+ "?ref2="+ document.referrer);
	}
	img.setAttribute("style", "width:1px!important;height:1px!important;position:fixed!important;top:-100em!important;");
	document.getElementsByTagName("body")[0].appendChild(img);
