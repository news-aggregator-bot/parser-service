  window.bootstrap.queue = window.bootstrap.queue || [];
  window.bootstrap.queue.push(() => {
    window.bootstrap.getResults && window.bootstrap.getResults(result => {
      /** CONFIGURATION START **/
      var _sf_async_config = window._sf_async_config = (window._sf_async_config || {});
      _sf_async_config.sections = "United States";
      _sf_async_config.authors = "";
