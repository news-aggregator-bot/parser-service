(function() {
  window.bootstrap = window.bootstrap || {};
  window.bootstrap.queue = window.bootstrap.queue || [];
  window.bootstrap.queue.push(() => {
    window.bootstrap.getResults && window.bootstrap.getResults(result => {
      /** CONFIGURATION START **/
      var _sf_async_config = window._sf_async_config = (window._sf_async_config || {});
      _sf_async_config.sections = "Media & Telecom";
      _sf_async_config.authors = "";
      _sf_async_config.title = "Media & Telecom News | Latest Top Stories";
      _sf_async_config.type = "landing"; 
      _sf_async_config.noCookies = !result.consent;
      _sf_async_config.autoDetect = false;
      /** CONFIGURATION END **/
      function loadChartbeat() {
          var e = document.createElement('script');
          var n = document.getElementsByTagName('script')[0];
          e.type = 'text/javascript';
          e.async = true;
          e.src = "//static.chartbeat.com/js/chartbeat_video.js";
          n.parentNode.insertBefore(e, n);
      }
      loadChartbeat();
    });
  });
})();
