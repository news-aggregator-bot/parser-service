jQuery('#w6').yiiActiveForm([{"id":"newsaddform-files","name":"files","container":".field-newsaddform-files","input":"#newsaddform-files","error":".help-block.help-block-error","validate":function (attribute, value, messages, deferred, $form) {yii.validation.file(attribute, messages, {"message":"Загрузка файла не удалась.","skipOnEmpty":true,"mimeTypes":[],"wrongMimeType":"Разрешена загрузка файлов только со следующими MIME-типами: .","extensions":["doc","xls","docx","xlsx","zip","rar","pdf","odt","ods"],"wrongExtension":"Разрешена загрузка файлов только со следующими расширениями: doc, xls, docx, xlsx, zip, rar, pdf, odt, ods.","maxFiles":1,"tooMany":"Вы не можете загружать более 1 файла."});}},{"id":"newsaddform-photos","name":"photos[]","container":".field-newsaddform-photos","input":"#newsaddform-photos","error":".help-block.help-block-error","validate":function (attribute, value, messages, deferred, $form) {yii.validation.file(attribute, messages, {"message":"Загрузка файла не удалась.","skipOnEmpty":true,"mimeTypes":[],"wrongMimeType":"Разрешена загрузка файлов только со следующими MIME-типами: .","extensions":["png","jpg","jpeg","heic","heif"],"wrongExtension":"Разрешена загрузка файлов только со следующими расширениями: png, jpg, jpeg, heic, heif.","maxFiles":20,"tooMany":"Вы не можете загружать более 20 файлов."});}},{"id":"newsaddform-text","name":"text","container":".field-newsaddform-text","input":"#newsaddform-text","error":".help-block.help-block-error","validate":function (attribute, value, messages, deferred, $form) {yii.validation.required(value, messages, {"message":"Необходимо заполнить «Текст новости»."});yii.validation.string(value, messages, {"message":"Значение «Текст новости» должно быть строкой.","skipOnEmpty":1});yii.validation.string(value, messages, {"message":"Значение «Текст новости» должно быть строкой.","min":5,"tooShort":"Значение «Текст новости» должно содержать минимум 5 символов.","skipOnEmpty":1});}},{"id":"newsaddform-captcha","