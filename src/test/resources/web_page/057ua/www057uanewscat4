  <script>jQuery(function ($) {
(function ($, idsPlaceholder) {
    $("form[data-form=rubrics-list]").on("submit", function () {
        var $this = $(this),
            rubricIds = $this.find("input[name=rubricIds]:checked").map(function () {
                return parseInt($(this).val(), 10);
            }).get(),
            formAction = $this.data("urlTemplate");
        rubricIds.sort(function (a, b) {
            return a - b;
        });
        $this.find("input[name='rubricIds[]']").remove();
        formAction = rubricIds.length > 0 ?
            formAction.replace(idsPlaceholder, rubricIds.join()) :
            $this.prop("action");
        $this.prop("action", formAction);
    });
})(jQuery, "{rubricIds}");
